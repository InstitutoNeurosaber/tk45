    "netlify:build": "npm run netlify:clean && npm run build && cd netlify/functions && npm install",
    "prepare-deploy": "node scripts/prepare-deploy.js",
    "deploy:preview": "npm run prepare-deploy && netlify deploy",
    "deploy:prod": "npm run prepare-deploy && netlify deploy --prod"
